<?xml version="1.0" encoding="UTF-8"?>

<service name="helloAgainService">
	<description>
	Katie tests hello world with pojos
	</description>
	
	<parameter name="ServiceClass" locked="false">
		org.sakaiproject.axis2.helloagain
	</parameter>
	
	<messageReceivers>
        <messageReceiver mep="http://www.w3.org/2004/08/wsdl/in-only"
